<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="bancoDeSolucao.FAQ" table="faq">
      
      <id name="idFaq" column="id_faq">
        <generator class="identity"/>
      </id>
      
    <property name="pergunta" column="pergunta"/>
    <property name="resposta" column="resposta"/>
    <property name="tipo" column="tipo"/>
    <property name="subtipo" column="subtipo"/>

	</class>
</hibernate-mapping>
