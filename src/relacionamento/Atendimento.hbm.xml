<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="relacionamento.Atendimento" table="atendimento">
      
    <id name="id" column="id_atendimento">
    	<generator class="identity"/>
    </id>
      
    <property name="idUnidadeSuporte" column="idUnidadeSuporte"/>
    <property name="idUnidadeSolicitante" column="idUnidadeSolicitante"/>
    
  </class>
</hibernate-mapping>
