<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="relacionamento.ChamadoAtendido" table="chamado_atendido">
      
    <id name="idChamado" column="id_chamado">
    </id>
      
    <property name="unidadeSolicitante" column="unidade_solicitante"/>
    <property name="unidadeAtendente" column="unidade_atendente"/>
    
  </class>
</hibernate-mapping>
