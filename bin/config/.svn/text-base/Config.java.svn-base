package config;


public class Config {
	
	//email_config
	public static final String SMTP_HOSTNAME = "SMTP_HOSTNAME";
	public static final String PASSWORD = "PASSWORD";
	public static final String DOMAIN = "DOMAIN";
	public static final String USERNAME = "USERNAME";
	
	public static final String URL_LDAP = "URL_LDAP";
	public static final String EMAIL_NOTIFICACOES = "EMAIL_NOTIFICACOES";
	public static final String NOTIFICACAO = "NOTIFICACAO";
	public static final String USAR_LOGIN_LDAP = "USAR_LOGIN_LDAP";
	public static final String TESTAR_EXISTENCIA_PATRIMONIO = "TESTAR_EXISTENCIA_PATRIMONIO";
	public static final String SENHA_ADMINISTRADOR = "SENHA_ADMINISTRADOR";
	public static final String CRIAR_SUBTIPO_AUTOMATICAMENTE = "CRIAR_SUBTIPO_AUTOMATICAMENTE";
	public static final String PRAZO_DO_SUBTIPO_DEFAULT = "PRAZO_DO_SUBTIPO_DEFAULT";
	
	

	public String id;
	public String valor;

	public Config() {
		super();
	}

	public Config(String id, String valor) {
		super();
		this.id = id;
		this.valor = valor;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getValor() {
		return valor;
	}

	public void setValor(String valor) {
		this.valor = valor;
	}

}
