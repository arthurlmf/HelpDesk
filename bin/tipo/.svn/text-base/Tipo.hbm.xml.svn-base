<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="tipo.Tipo" table="tipo">
   
     <composite-id name="chaveTipo" class="tipo.PrimaryKeyTipo" access="property" >  
          <key-property name="idUnidade" column="id_unidade" />  
          <key-property name="nomeTipo" column="nome_tipo" />  
     </composite-id>  
       
  </class>
</hibernate-mapping>
